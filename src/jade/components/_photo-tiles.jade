.content
    .row
        .small-12.medium-8.columns
            h2 Фото
        .small-12.medium-4.columns.medium-text-right
            a.button(href='#') Все фото &nbsp;<i class="fa fa-long-arrow-right"></i>

    section.row.small-up-2.medium-up-3.large-up-5(data-equalizer='news', data-equalize-by-row='true')
        each name in ['Новый сайт компании 1', 'Новый сайт компании 2', 'Новый сайт компании 3', 'Новый сайт компании 4', 'Новый сайт компании 5', 'Новый сайт компании 6']
            .column
                .callout(data-equalizer-watch='photo')
                    p
                        img(src='./images/image.svg', alt='Видео', style='background:#bbb;width:100%;height:150px;')
                    p
                        a(href='#')= name