.content
    .row
        .small-12.medium-8.columns
            h2 Фото
        .small-12.medium-4.columns.medium-text-right
            a.button(href='#') Все фото &nbsp;<i class="fa fa-long-arrow-right"></i>

    section.row.small-up-1.medium-up-2.large-up-4(data-equalizer='photo', data-equalize-by-row='true')
        each name in [1, 2, 'ООО Промышленные Энергосистемы', 4]
            .column
                .callout(data-equalizer-watch='photo')
                    p
                        img(src='./images/image.svg', alt='Фото', style='background:#bbb;width:100%;height:150px;')
                    p
                        a(href='#')= 'Новый сайт компании ' + name