.content
    .row
        .small-12.medium-8.columns
            h2 Бренды
        .small-12.medium-4.columns.medium-text-right
            a.button.hollow(href='#') Все Бренды &nbsp;
                i.fa.fa-long-arrow-right
    .row.small-up-2.medium-up-4.large-up-8
        - var n = 0
        while n < 8
            .column
                p: img(src='./images/image.svg' style='width:100%; height:75px; background:#FEF141;' alt=n)
            - n++