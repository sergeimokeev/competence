.padding-top-12.padding-bottom-6
    .grid-x.grid-padding-x
        .small-24.medium-16.cell
            h2 Фото
        .small-24.medium-8.cell.medium-text-right
            a.button(href='#') Все фото &nbsp;<i class="fa fa-long-arrow-right"></i>

    section.grid-x.grid-padding-x.small-up-1.medium-up-2.large-up-4(data-equalizer='photo', data-equalize-by-row='true')
        each name in [1, 2, 'ООО Промышленные Энергосистемы', 4]
            .cell.margin-bottom-6(data-equalizer-watch='photo')
                a(href='#'): img.margin-bottom-3(src='./i/photo.svg', alt='Фото')
                a(href='#')= 'Новый сайт компании ' + name